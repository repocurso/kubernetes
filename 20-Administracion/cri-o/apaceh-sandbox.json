{
  "metadata": {
    "name": "apache-sandbox",
    "namespace": "default"
  },
  "linux": {
    "resources": {
      "cpuPeriod": 100000,
      "cpuQuota": 50000,
      "cpuShares": 1024,
      "memoryLimitInBytes": 536870912
    }
  },
  "pid": {
    "limit": 2048
  },
  "sandboxId": "apache-sandbox",
  "hostname": "apache-sandbox",
  "logDirectory": "/var/log/apache-sandbox",
  "dnsConfig": {
    "nameservers": ["8.8.8.8", "8.8.4.4"]
  },
  "portMappings": [
    {
      "containerPort": 80,
      "hostPort": 8080,
      "protocol": "tcp"
    }
  ],
  "labels": {
    "app": "apache"
  }
}
